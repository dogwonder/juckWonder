
<link rel="icon" type="image/png" sizes="16x16" href="./images/fav/favicon-16x16.png">
<link rel="mask-icon" sizes="any" href="./images/logotype.svg" color="#14d5de">
<link rel="shortcut icon" href="./images/fav/favicon.ico" type="image/x-icon">
<link rel="apple-touch-icon" sizes="192x192" href="./images/fav/apple-touch-icon.png" />
<link rel="manifest" href="./images/fav/manifest.json">
<meta name="apple-mobile-web-app-title" content="Twitter" />
<meta name="apple-mobile-web-app-status-bar-style" content="white" />
<meta name="msapplication-TileColor" content="#14d5de">
<meta name="theme-color" content="#14d5de">
<meta property="og:url" content="{{ siteTitle }}" />
<meta property="og:site_name" content="" />
<meta property="og:locale" content="en_GB" />
<meta property="og:title" content="{% if pageTitle %}{{ siteTitle }} - {{ pageTitle }}{% else %}{{ siteTitle }}{% endif %}">
<meta property="og:description" content="{{ siteDesc }}">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="630">
<meta property="og:image" content="{{ siteImage }}">
<meta property="og:url" content="{{ siteUrl }}">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="{% if pageTitle %}{{ siteTitle }} - {{ pageTitle }}{% else %}{{ siteTitle }}{% endif %}">
<meta name="twitter:description" content="{{ siteDesc }}">
<meta name="twitter:image" content="{{ siteImage }}">